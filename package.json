{
  "name": "vite-react-mymodal",
  "private": false,
  "version": "1.0.2",
  "description": "A modal component for React",
    "keywords": [
        "react",
        "modal",
        "component"
    ],
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
